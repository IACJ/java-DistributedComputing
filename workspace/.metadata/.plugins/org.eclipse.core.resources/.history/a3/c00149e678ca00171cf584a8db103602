package com.acj.sock;

import java.io.IOException;
import java.net.InetAddress;
import java.net.Proxy;
import java.net.Socket;
import java.net.SocketException;
import java.net.SocketImpl;
import java.net.UnknownHostException;

public class MyStreamSocket extends Socket {

	public MyStreamSocket() {
		// TODO Auto-generated constructor stub
	}

	public MyStreamSocket(Proxy proxy) {
		super(proxy);
		// TODO Auto-generated constructor stub
	}

	public MyStreamSocket(SocketImpl impl) throws SocketException {
		super(impl);
		// TODO Auto-generated constructor stub
	}

	public MyStreamSocket(String host, int port) throws UnknownHostException, IOException {
		super(host, port);
		// TODO Auto-generated constructor stub
	}

	public MyStreamSocket(InetAddress address, int port) throws IOException {
		super(address, port);
		// TODO Auto-generated constructor stub
	}

	public MyStreamSocket(String host, int port, InetAddress localAddr, int localPort) throws IOException {
		super(host, port, localAddr, localPort);
		// TODO Auto-generated constructor stub
	}

	public MyStreamSocket(InetAddress address, int port, InetAddress localAddr, int localPort) throws IOException {
		super(address, port, localAddr, localPort);
		// TODO Auto-generated constructor stub
	}

}
