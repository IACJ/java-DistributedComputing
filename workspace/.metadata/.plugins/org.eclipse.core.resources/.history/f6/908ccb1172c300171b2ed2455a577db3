package com.acj.rmi.server;

public class Student { 
	
	private Long id; 
    private String name; 
    private Integer grade; 
 
    public Student(Long i, String name, Integer grade) {
		super();
		this.id = i;
		this.name = name;
		this.grade = grade;
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Integer getGrade() {
		return grade;
	}
	public void setGrade(Integer grade) {
		this.grade = grade;
	}

     
}
