import java.io.IOException;

public class HelloWorld {

	public static void main(String[] args) {
		System.out.println("hello world!");
		int test = 9;
		try {
			test = System.in.read();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.println(test);
	}

}
