package com.acj.streamsocket;

import java.net.ServerSocket;

public class Server {
	
	public static void main(String[] args) {
		try{
			final int PORT = 1234;
			ServerSocket serverSocket = new ServerSocket(PORT);
			MyStreamSocket mySocket = serverSocket.accept();
			
		}catch(Exception e){
			e.printStackTrace();
		}
	}

}
